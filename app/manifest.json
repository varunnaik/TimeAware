{
    "name": "TimeAware",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "__MSG_appDescription__",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "default_locale": "en",
    "background": {
        "scripts": [
            "scripts/storage.js",
            "scripts/timeaware.js",
            "scripts/background.js"
        ]
    },
  "options_page": "options.html",
  "permissions": [
    "tabs",
    //"http://*/*",
    //"https://*/*",
    "storage"
  ]
}
